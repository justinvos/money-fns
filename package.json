{
  "name": "money-fns",
  "version": "0.4.2",
  "description": "Next-generation Money library (FP, ESM, BigInt).",
  "keywords": [
    "money",
    "amount",
    "currency",
    "fp",
    "functional",
    "pure"
  ],
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "lib/types/index.d.ts",
  "scripts": {
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=lib/esm/index.js",
    "build:cjs": "esbuild src/index.ts --bundle --format=cjs --outfile=lib/cjs/index.js",
    "build:types": "tsc",
    "test": "node ./src/tests/general.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/justinvos/money-fns.git"
  },
  "author": "Justin Vos",
  "license": "MIT",
  "devDependencies": {
    "colors": "^1.4.0",
    "esbuild": "^0.9.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {}
}
